---
import MeBentoGrid from "./MeBentoGrid";
import { getImage } from "astro:assets";
import collegeLogo from "../data/djsce-logo.avif";
import aryaLogo from "../data/arya.avif";
import codeAIlogo from "../data/codeai.avif";
import varakLogo from "../data/varak.avif";
import gssocLogo from "../data/gssoc.avif";
import winIcon from "../data/winIcon.png";

const { beeImage } = Astro.props;

const optimizedImages = {
    beeImage: beeImage,
    collegeLogo: await getImage({
        src: collegeLogo,
        format: "avif",
        width: 113,
        quality: 100,
    }),
    aryaLogo: await getImage({ src: aryaLogo, format: "avif", width: 113, quality: 100 }),
    codeAIlogo: await getImage({ src: codeAIlogo, format: "avif", width: 113, quality: 100 }),
    varakLogo: await getImage({ src: varakLogo, format: "avif", width: 113, quality: 100 }),
    gssocLogo: await getImage({ src: gssocLogo, format: "avif", width: 113, quality: 100 }),
    winIcon: await getImage({ src: winIcon, format: "png", width: 100, quality: 100 }),
};
---

<MeBentoGrid client:visible optimizedImages={optimizedImages} />
